{
  "name": "jpv/poker-faces",
  "version": "0.1.2",
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.1.3",
    "symfony/console": "4.2.*"
  },
  "require-dev": {
    "codeception/base": "^2.5",
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  }
}
